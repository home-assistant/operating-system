PART_KERNEL_0=/dev/disk/by-partuuid/0d3e0000-05
PART_KERNEL_1=/dev/disk/by-partuuid/0d3e0000-07
PART_SYSTEM_0=/dev/disk/by-partuuid/0d3e0000-06
PART_SYSTEM_1=/dev/disk/by-partuuid/0d3e0000-08
